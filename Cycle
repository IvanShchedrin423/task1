using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Cycle
{
    internal class Program
    {
        static void Main(string[] args)
        {
            // for позволяет эту задачу выполнить в 4 строчки, while потребовал бы больше строк
            for (int i = 5; i < 100; i += 7)
            {
                Console.WriteLine(i);
            }
        }
    }
}
